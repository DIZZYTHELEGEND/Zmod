			script.Parent.Parent.Parent.Parent.TextLabel.Backgrounds["Anime shit"].Anim2.Visible = true
			script.Parent.Parent.Parent.Parent.TextLabel.Backgrounds["Anime shit"].Anim1.Visible = false
			script.Parent.Parent.Parent.Parent.TextLabel.Backgrounds["Anime shit"].Anim4.Visible = false
			script.Parent.Parent.Parent.Parent.TextLabel.Backgrounds["Anime shit"].Anim3.Visible = false
			script.Parent.Parent.Parent.Parent.TextLabel.Backgrounds.MrThundee.Visible = false
			script.Parent.Parent.Parent.Parent.TextLabel.Backgrounds.bloody.Visible = false
			-- Do something when the button is toggled on
