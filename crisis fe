coroutine.wrap(RVJB_fake_script)()
local function HZMGRQK_fake_script() -- lines.LocalScript 
	local script = Instance.new('LocalScript', lines)

	-- Get the TextBox and TextLabel objects
	local textBox = script.Parent.Parent.Parent.executor.code_box
	local lineNumbers = script.Parent.Parent.lines
	
	-- Define a function to update the line numbers
	local function updateLineNumbers()
		-- Split the text into individual lines
		local lines = string.split(textBox.Text, "\n")
	
		-- Update the TextLabel to show the number of lines
		lineNumbers.Text = ""
		for i, line in ipairs(lines) do
			lineNumbers.Text = lineNumbers.Text .. i .. "\n"
		end
	end
	
	-- Call the updateLineNumbers function whenever the TextBox text changes
	textBox.Changed:Connect(updateLineNumbers)
	
	-- Call the updateLineNumbers function initially to set the line numbers
	updateLineNumbers()
	
end
coroutine.wrap(HZMGRQK_fake_script)()
local function RKHOD_fake_script() -- Options.LocalScript 
	local script = Instance.new('LocalScript', Options)

	local toggleState = false
	
	script.Parent.MouseButton1Click:Connect(function()
		toggleState = not toggleState
		if toggleState then
			script.Parent.Parent.MenuFrame.Visible = true
			script.Parent.Parent.bgframe.Visible = false
			script.Parent.Parent.bgframe.animeop.Visible = false
			-- Do something when the button is toggled on
		else
			script.Parent.Parent.MenuFrame.Visible = false
			-- Do something when the button is toggled off
		end
	end)
	
end
coroutine.wrap(RKHOD_fake_script)()
local function GUNXEPH_fake_script() -- Clear.RippleEffect 
