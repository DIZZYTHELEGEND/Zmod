local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local StarterGui = game:GetService("StarterGui")

local playerName = Players.LocalPlayer.Name
local playerRank = "Member" -- default rank

-- Check the player's username and set the rank accordingly
if playerName == "Di33le2" then
    playerRank = "Owner"
elseif playerName == "tpskingbrotha" or playerName == "xorobf" then
    playerRank = "Powerful admin"
elseif playerName == "nooneyte0wyfiushiourfihrsfiuhrfjrsofofrhfhreufhrufhrfhruhfuf" then
    playerRank = "Mod"
elseif playerName == "idonthacklol101ns" then
    playerRank = "Developer"
elseif playerName == "ZmodOnTop" then
    playerRank = "Advertiser"
elseif playerName == "FlyingGorilla936456" or playerName == "Altsarecooleh" then
    playerRank = "beta tester"
end

local welcomeMessage = "Zirconia/Zmod Loaded. Welcome, " .. playerName .. "! Your rank is " .. playerRank .. ". Say cmds to get started. And join the server dsc.gg/5gShAQvmU"

-- Create a notification using StarterGui
StarterGui:SetCore("SendNotification", {
    Title = "Welcome to the game!";
    Text = welcomeMessage;
    Duration = 5; -- Set the duration in seconds
})

-- Optionally, you can also print the welcome message in the output for debugging purposes
print(welcomeMessage)












game.Players.LocalPlayer.Chatted:Connect(handleCommand)









-- Whitelisted usernames
local whitelist = {
    "Joeyplayzgame_stoiu",
    "Di33le2",
    "tpskingbrotha",
    "Unlocsir",
    "Pikachu12342334",
    "idonthacklol101ns",
    "SBSJDBSJDGDB",
    "xorobf",
    "ZmodOnTop",
    "krnltestXD",
    "FlyingGorilla936456",
    "Altsarecooleh",
    "Me_LScriptLOL"
    
    
    
    

}

local function checkWhitelist()
    for _, player in ipairs(game.Players:GetPlayers()) do
        local playerName = player.Name
        local isWhitelisted = false

        for _, whitelistedName in ipairs(whitelist) do
            if playerName == whitelistedName then
                isWhitelisted = true
                break
            end
        end

        if not isWhitelisted then
            player:Kick("You are not whitelisted. please reframe from using this script. (me rn (      >:(     )")
        end
    end
end

while true do
    checkWhitelist()
    wait(1) -- Adjust the interval (in seconds) between each whitelist check
end






