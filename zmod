    -- Get the current server place ID
    local currentPlaceId = game.PlaceId
    local currentJobId = game.JobId
    

    -- Teleport the player to the same server
    ts:TeleportToPlaceInstance(currentPlaceId, currentJobId, p)
    
    
    
