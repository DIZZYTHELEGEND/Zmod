local function executeLoadstring(loadstringCode)
    local success, result = pcall(function()
        return loadstring(loadstringCode)()
    end)

    if not success then
        warn("Error executing loadstring:", result)
    end
end

local function handleCommand(message)

    local lowercaseMessage = string.lower(message)

    if lowercaseMessage == ".cmds" or lowercaseMessage == ".cmd" or lowercaseMessage == ".cm" or lowercaseMessage == ".cmmds" then
        executePrints()
    end

    if lowercaseMessage == ".iy" then
        local loadstringCode = "loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source', true))()"
        executeLoadstring(loadstringCode)
    elseif lowercaseMessage == ".cmdx" then
        local loadstringCode = "loadstring(game:HttpGet('https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source', true))()"
        executeLoadstring(loadstringCode)
    elseif lowercaseMessage == ".fat" then
        local loadstringCode = "loadstring(game:HttpGet('https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua', true))()"
        executeLoadstring(loadstringCode)
    elseif lowercaseMessage == ".cmdv1" then
        local loadstringCode = "loadstring(game:HttpGet('https://raw.githubusercontent.com/quivings/KAH/main/CMD%20v1.lua', true))()"
        executeLoadstring(loadstringCode)
    elseif lowercaseMessage == ".cmdy" or lowercaseMessage == ".perm/c" then
        local loadstringCode = "loadstring(game:HttpGet('https://raw.githubusercontent.com/quivings/KAH/main/CMD-Y%20(v1.56).lua', true))()"
        executeLoadstring(loadstringCode)
        
elseif lowercaseMessage == ".house" then
local Character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
	if Character then
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-30.1969719, 8.22999954, 72.12043, -0.998412967, -2.57047859e-08, 0.0563161708, -2.74235497e-08, 1, -2.97470795e-08, -0.0563161708, -3.12442587e-08, -0.998412967)
		end
		
		
    elseif lowercaseMessage == ".spawn" then
        local loadstringCode = "loadstring(game:HttpGet('https://raw.githubusercontent.com/DIZZYTHELEGEND/Zmod/exptable/spawn', true))()"
        executeLoadstring(loadstringCode)		
		
		
		
    elseif lowercaseMessage == ".tptool" then
        local tool = Instance.new("Tool")
        tool.RequiresHandle = false
        tool.Name = "Zmod.Tptool"
        tool.Activated:Connect(function()
            local mouse = game.Players.LocalPlayer:GetMouse()
            local pos = mouse.Hit + Vector3.new(0, 2.5, 0)
            pos = CFrame.new(pos.X, pos.Y, pos.Z)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
        end)
        tool.Parent = game.Players.LocalPlayer.Backpack
   elseif lowercaseMessage == ".rj"  or lowercaseMessage == ".rejoin" or lowercaseMessage == ".rej" then
