local function teleportPlayer(player, x, y, z, rX, rY, rZ, sX, sY, sZ, wX, wY, wZ)
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.RootPart.CFrame = CFrame.new(x, y, z) * CFrame.Angles(rX, rY, rZ) * CFrame.new(sX, sY, sZ) * CFrame.Angles(wX, wY, wZ)
        end
    end
end

-- Example usage:
local player = game.Players.LocalPlayer -- Replace with the target player
teleportPlayer(player, -29, 0.0999880284, -25.5900097, -1, 0, 0, 0, 1, 0, 0, 0, -1)
