       local function Chat(msg)

            game.Players:Chat(msg)
        end

while true do
    Chat("unpunish all")
    Chat("unjail all")
    Chat("thaw all")
    Chat("speed all 16")
    Chat("unname all")
    Chat("ungear all")
    Chat("unblind all")

    Chat("unflash")
    Chat("h SERVER IS PROTECTED BY ZMOD")
    wait(0.5)
end
